{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "height": 500,
  "width": 500,
  "title": "Crude Suicide rates vs GDP per Country (2016)",
  "data": {
    "url": "https://raw.githubusercontent.com/FIT3179/Vega-Lite/main/4_interactive_scatter_plot/data/COVID_19_10_Oct_2020.csv"
  },
  "mark": "circle",
  "encoding": {
    "x": {
      "field": "Confirmed",
      "type": "quantitative",
      "title": "Confirmed Cases"
    },
    "y": {
      "field": "Deaths",
      "type": "quantitative"
    },
    "color": {
      "field": "Continent",
      "type": "nominal"
    },
    "size": {
      "field": "Population",
      "type": "quantitative"
    }
  }
}
